<div class="page-header">
  <%= link_to subsubcategories_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_subsubcategory_path(@subsubcategory), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show subsubcategory</h1>
</div>

<dl class="dl-horizontal">
  <dt>Parent:</dt>
  <dd><%= @subsubcategory.parent %></dd>

  <dt>Name:</dt>
  <dd><%= @subsubcategory.name %></dd>

</dl>
